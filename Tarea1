package tarea;

import java.util.Scanner;

/**
 *
 * @author alejo
 */
public class Tarea {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
    Scanner ObjetoEntrada=new Scanner(System.in);
    
    int lista[] = new int[5];
    
    System.out.println("ingrese su lista de 5 numeros");
    System.out.println(" ");
    for (int i=0;i<lista.length;i++){
        System.out.println("Inserte un numero de la lista");
        lista[i]=ObjetoEntrada.nextInt();
    }
    System.out.println(" ");
    System.out.println("Su lista es la siguiente:");
    System.out.println(" ");
    for (int lectura:lista){
        System.out.println(+lectura);
    }
    }
}
   
